{
    "name": "icare/icarepontianak",
    "description": "iCare Pontianak API",
    "license": "MIT",
    "require": {
        "php": ">=5.6",
        "firebase/php-jwt": "^5.0",
        "monolog/monolog": "^1.17",
        "slim/slim": "^3.1",
        "tuupola/slim-jwt-auth": "^3.4",
        "vlucas/phpdotenv": "^3.4"
    },
    "require-dev": {
        "phpunit/phpunit": ">=5.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "config": {
        "process-timeout": 0,
        "sort-packages": true
    },
    "scripts": {
        "start": "php -S localhost:8080 -t public",
        "test": "phpunit"
    }
}